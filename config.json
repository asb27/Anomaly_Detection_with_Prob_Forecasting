{
  "data_processor": {
    "variables": ["Consumption", "temperature", "humidity", "day_of_week_sin", "day_of_week_cos", "is_weekend", "Consumption_MA_6", "Consumption_lag_1"],
    "moving_averages": {
      "Consumption": [6, 24]
    },
    "lags": {
      "Consumption": [1]
    }
  },

   "forecasting": {
        "type": "quantile_regression",
        "quantiles": [0.0001, 0.001, 0.005, 0.01, 0.05, 0.1, 0.5, 0.9, 0.99, 0.999, 0.9999],
        "alpha": 0.0
    },

  "anomalies": {
    "types": ["anomaly_tv", "anomaly_evacuation"],
    "periods": {
      "anomaly_tv": [
        ["2020-02-05 10:20", "2020-02-05 14:20"]
      ],
        "anomaly_evacuation": [
            ["2020-03-05 10:20", "2020-03-05 16:20"]
        ]
    },
    "classes": {
      "anomaly_tv": "AnomalyTv",
      "anomaly_evacuation": "AnomalyEvacuation",
      "anomaly_peak": "AnomalyPeak"
    }
  },

  "detection_methods": {
    "methods": ["detection_with_quantile", "detection_with_threshold"],
    "parameters": {
      "detection_with_quantile": [
        {"upper_quantile": "0.99","lower_quantile": "0.01"}
      ],
        "detection_with_threshold": [
            {"upper_quantile": "0.99","lower_quantile": "0.01", "threshold": 0.1}
        ]
    }
  }
}
